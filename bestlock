UndergroundAAResolver.Name = "Underground AA Resolver"
UndergroundAAResolver.Parent = Frame
UndergroundAAResolver.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
UndergroundAAResolver.BorderColor3 = Color3.fromRGB(170, 85, 255)
UndergroundAAResolver.Position = UDim2.new(0.511891782, 0, 0.130281702, 0)
UndergroundAAResolver.Size = UDim2.new(0, 191, 0, 34)
UndergroundAAResolver.Font = Enum.Font.Code
UndergroundAAResolver.Text = "Underground AA Resolver"
UndergroundAAResolver.TextColor3 = Color3.fromRGB(255, 255, 255)
UndergroundAAResolver.TextSize = 14.000
MouseButton1Click:Connect(function()
	local RunService = game:GetService("RunService")

	RunService.Heartbeat:Connect(function()
		pcall(function()
			for i,v in pairs(game.Players:GetChildren()) do
				if v.Name ~= game.Players.LocalPlayer.Name then
					local hrp = v.Character.HumanoidRootPart
					hrp.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)    
					hrp.AssemblyLinearVelocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)   
				end
			end
		end)
	end)
end)
